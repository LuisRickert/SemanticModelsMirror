

@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:io.catenax.specialCharacteristics.specification:1.0.0#> .

:specification a samm:Aspect ;
   samm:preferredName "SpecialCharacteristicMeasurment"@en ;
   samm:description "Contains the Measurment for a special characteristic"@en ;
   samm:properties ( :testLocation :measurementType :purpose :classification :revisionIndex :customerPartID :characteristicID :expectedMeasurement ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:testLocation a samm:Property ;
   samm:characteristic :TestLocationCharacteristic ;
   samm:exampleValue "inHouse" .

:measurementType a samm:Property ;
   samm:characteristic :MeasurementType ;
   samm:exampleValue "qualitative" .

:purpose a samm:Property ;
   samm:characteristic :PurposeCharacteristics ;
   samm:exampleValue "Traceability" .

:classification a samm:Property ;
   samm:characteristic :ClassificationCharacteristics ;
   samm:exampleValue "Safety" .

:revisionIndex a samm:Property ;
   samm:characteristic :ID ;
   samm:exampleValue "1"^^xsd:positiveInteger .

:customerPartID a samm:Property ;
   samm:characteristic :String .

:characteristicID a samm:Property ;
   samm:characteristic :String .

:expectedMeasurement a samm:Property ;
   samm:characteristic :ExpectedMeasurementCharacteristic .

:TestLocationCharacteristic a samm-c:Enumeration ;
   samm:dataType xsd:string ;
   samm-c:values ( "inHouse" "Production-Supplier" "Production-Sub-Supplier" ) .

:MeasurementType a samm-c:Enumeration ;
   samm:dataType xsd:string ;
   samm-c:values ( "categorical" "ordinal" "attributive" "quantitative" "qualitative" ) .

:PurposeCharacteristics a samm-c:Enumeration ;
   samm:dataType xsd:string ;
   samm-c:values ( "Traceability" "BTRV" ) .

:ClassificationCharacteristics a samm-c:Enumeration ;
   samm:dataType xsd:string ;
   samm-c:values ( "Functional" "Safety" "Legal" ) .

:ID a samm:Characteristic ;
   samm:dataType xsd:positiveInteger .

:String a samm:Characteristic ;
   samm:dataType xsd:string .

:ExpectedMeasurementCharacteristic a samm-c:List ;
   samm:dataType :ExpectedMeasurement .

:ExpectedMeasurement a samm:Entity ;
   samm:properties ( :targetValue :upperThreshold :lowerThreshold ) .

:targetValue a samm:Property ;
   samm:characteristic :ValueCharacteristic .

:upperThreshold a samm:Property ;
   samm:characteristic :Characteristic1 .

:lowerThreshold a samm:Property ;
   samm:characteristic :Characteristic2 .

:ValueCharacteristic a samm:Characteristic ;
   samm:dataType :ValueSI .

:Characteristic1 a samm:Characteristic ;
   samm:dataType :ValueSI .

:Characteristic2 a samm:Characteristic ;
   samm:dataType :ValueSI .

:ValueSI a samm:Entity ;
   samm:properties ( :value :unit ) .

:value a samm:Property ;
   samm:characteristic :ValueCharacteristics .

:unit a samm:Property ;
   samm:characteristic :UnitCharacteristic .

:ValueCharacteristics a samm:Characteristic ;
   samm:dataType xsd:float .

:UnitCharacteristic a samm:Characteristic ;
   samm:dataType samm:curie .

