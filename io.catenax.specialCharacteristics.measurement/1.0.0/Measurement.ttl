

@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:io.catenax.specialCharacteristics.measurement:1.0.0#> .
@prefix bpn: <urn:samm:io.catenax.shared.business_partner_number:2.0.0#> .
@prefix samm-u: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix serialPart: <urn:samm:io.catenax.serial_part:3.0.0#> .

:Measurement a samm:Aspect ;
   samm:preferredName "SpecialCharacteristicMeasurment"@en ;
   samm:description "Contains the Measurment for a special characteristic"@en ;
   samm:properties ( serialPart:localIdentifiers bpn:bpnlProperty :customerPartID :characteristicID :results :measurmentType ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:customerPartID a samm:Property ;
   samm:preferredName "customerPartID"@en ;
   samm:description "This number is the identification of the Part"@en ;
   samm:characteristic :CharacteristicCustomerPartID ;
   samm:exampleValue "BPN0000000" .

:characteristicID a samm:Property ;
   samm:preferredName "characteristicID"@en ;
   samm:description "Identification of the special characteristic in in relation of the specified part"@en ;
   samm:characteristic :CharacteristicCustomerPartID ;
   samm:exampleValue "S1" .

:results a samm:Property ;
   samm:preferredName "measurement_results"@en ;
   samm:description "List of measurementObjects"@en ;
   samm:characteristic :ResultList .

:measurmentType a samm:Property ;
   samm:preferredName "characteristicID"@en ;
   samm:description "Identification of the special characteristic in in relation of the specified part"@en ;
   samm:characteristic :MeasurementTypeCharacteristic ;
   samm:exampleValue "qualitative" .

:CharacteristicCustomerPartID a samm:Characteristic ;
   samm:preferredName "customerPartID_characteristic"@en ;
   samm:dataType xsd:string .

:ResultList a samm-c:List ;
   samm:preferredName "ResultList"@en ;
   samm:description "List of result objects"@en ;
   samm:dataType :MeasurmentObject .

:MeasurementTypeCharacteristic a samm-c:Enumeration ;
   samm:preferredName "MeasurementTypes"@en ;
   samm:description "Define the values for the possible Measurement types as enum. Values can be: Qualitative, Quantitative, Attributive, Ordinal, Categorical"@en ;
   samm:dataType xsd:string ;
   samm-c:values ( "qualitative" "quantitative" "attributive" "ordinal" "categorical" ) .

:MeasurmentObject a samm:Entity ;
   samm:preferredName "Measurmentobject"@en ;
   samm:description "This Entity represents a generic Measurment Object"@en ;
   samm:properties ( :measurmentDateTime ) .

:measurmentDateTime a samm:Property ;
   samm:preferredName "measurmentTimestamp"@en ;
   samm:description "Timestamp of the measurment."@en ;
   samm:characteristic samm-c:Timestamp ;
   samm:exampleValue "2024-09-30"^^xsd:dateTime .

:referenceValue a samm:Property ;
   samm:preferredName "Value of the reference measurement"@en ;
   samm:characteristic :MeasurementCharacteristic .

:ResistanceCharacteristic a samm:Characteristic ;
   samm:preferredName "Resistance Charactersitic"@en ;
   samm:dataType :ResistanceMeasurementEntity .

:ValueSI a samm:Entity ;
   samm:description "Value with an attached SI unit"@en ;
   samm:properties ( :value :unit ) .

:MeasurementCharacteristic a samm:Characteristic ;
   samm:preferredName "describe a value with an attached unit"@en ;
   samm:dataType :ValueSI .

:x a samm:Property ;
   samm:preferredName "x"@en ;
   samm:description "x value  of the measured point"@en ;
   samm:characteristic :CharacteristicPoint ;
   samm:exampleValue "12.1"^^xsd:float .

:FunctionalMeasurment a samm:Entity ;
   samm:extends :MeasurmentObject ;
   samm:properties ( :functionalValue ) .

:measurementPin a samm:Property ;
   samm:description "Pin to which this measurement is attributed to "@en ;
   samm:characteristic :PinCharacteristic .

:ValueCharacteristic a samm:Characteristic ;
   samm:dataType xsd:float .

:CharacteristicSpatial a samm:Characteristic ;
   samm:preferredName "Spatial Measurement"@en ;
   samm:description "Describes as Spatial Measurement"@en ;
   samm:dataType :PointInSpace .

:functionalValue a samm:Property ;
   samm:preferredName "Value"@en ;
   samm:description "measured functional value"@en ;
   samm:characteristic :CharacteristicFunctional ;
   samm:exampleValue true .

:location a samm:Property ;
   samm:preferredName "Pin Location"@en ;
   samm:description "location of the pin as described on the blueprint"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "D" .

:CharacteristicFunctional a samm:Characteristic ;
   samm:dataType xsd:boolean .

:unit a samm:Property ;
   samm:description "SI-Unit of the related value"@en ;
   samm:characteristic samm-c:UnitReference .

:ResistanceMeasurementEntity a samm:Entity ;
   samm:description "encapsulate all relevant information to identify pins for this measurement"@en ;
   samm:properties ( :measurementPin :referencePin :referenceValue :measuredValue ) .

:CharacteristicPoint a samm:Characteristic ;
   samm:preferredName "CharacteristicPoint"@en ;
   samm:description "descibing a point"@en ;
   samm:dataType xsd:float .

:referencePin a samm:Property ;
   samm:description "Pin which is the other measuring point for the resistance measuring"@en ;
   samm:characteristic :PinCharacteristic .

:ResistanceMeasuremtnObject a samm:Entity ;
   samm:extends :MeasurmentObject ;
   samm:preferredName "Resistance Measurement Object"@en ;
   samm:properties ( :resistanceMeasurement ) .

:SpatialMeasurment a samm:Entity ;
   samm:extends :MeasurmentObject ;
   samm:properties ( :spatialValue ) .

:customID a samm:Property ;
   samm:preferredName "Custom ID"@en ;
   samm:description "Identification of the Pin as defined on the blueprint"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "5" .

:z a samm:Property ;
   samm:preferredName "z"@en ;
   samm:description "z value of the measured point"@en ;
   samm:characteristic :CharacteristicPoint ;
   samm:exampleValue "69"^^xsd:float .

:PinCharacteristic a samm:Characteristic ;
   samm:preferredName "Pin Characeristic"@en ;
   samm:description "Characteristic to describe a reference PIn"@en ;
   samm:dataType :PinEntity .

:PointInSpace a samm:Entity ;
   samm:preferredName "PointinSpace"@en ;
   samm:description "x,y,z coordinates of a measured point in space"@en ;
   samm:properties ( :x :y :z ) .

:position a samm:Property ;
   samm:preferredName "PinPositon"@en ;
   samm:description "Position of the pin according to the blue print"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "01" .

:value a samm:Property ;
   samm:description "measured Value"@en ;
   samm:characteristic :ValueCharacteristic .

:y a samm:Property ;
   samm:preferredName "y"@en ;
   samm:description "y value of the measured point"@en ;
   samm:characteristic :CharacteristicPoint ;
   samm:exampleValue "420"^^xsd:float .

:measuredValue a samm:Property ;
   samm:preferredName "measured Value"@en ;
   samm:characteristic :MeasurementCharacteristic .

:resistanceMeasurement a samm:Property ;
   samm:preferredName "Resistance Measurement"@en ;
   samm:description "This object represents a resistance measurement where each measurement is taken between two pins in given a predefined voltage."@en ;
   samm:characteristic :ResistanceCharacteristic .

:PinEntity a samm:Entity ;
   samm:preferredName "Pin Entity"@en ;
   samm:properties ( :customID :location :position ) .

:spatialValue a samm:Property ;
   samm:preferredName "Value"@en ;
   samm:description "measured point in space"@en ;
   samm:characteristic :CharacteristicSpatial .

