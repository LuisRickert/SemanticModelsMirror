

@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:io.catenax.specialCharacteristics.measurement_simple:1.0.0#> .

:SimpleMeasurement a samm:Aspect ;
   samm:properties ( :measurementTimestamp :measurement ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:measurementTimestamp a samm:Property ;
   samm:preferredName "Measurement Timestamp"@en ;
   samm:description "Timestamp of the measurement"@en ;
   samm:characteristic :MeasurementTimestamp ;
   samm:exampleValue "2024-10-01"^^xsd:date .

:measurement a samm:Property ;
   samm:characteristic :MeasurementCharacteristic .

:MeasurementTimestamp a samm:Characteristic ;
   samm:preferredName "MeasurementTimestamp"@en ;
   samm:dataType xsd:date .

:MeasurementCharacteristic a samm:Characteristic ;
   samm:dataType :Measurement .

:Measurement a samm:Entity ;
   samm:properties ( :unit :value ) .

:unit a samm:Property ;
   samm:characteristic :UnitCharaceristic .

:value a samm:Property ;
   samm:characteristic :ValueCharacteristic .

:UnitCharaceristic a samm:Characteristic ;
   samm:dataType xsd:string .

:ValueCharacteristic a samm:Characteristic ;
   samm:dataType xsd:float .

