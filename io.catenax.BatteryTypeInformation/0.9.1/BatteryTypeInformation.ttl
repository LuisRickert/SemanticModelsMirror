

@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:io.catenax.BatteryTypeInformation:0.9.1#> .
@prefix ext-ptf: <urn:samm:io.catenax.part_type_information:1.0.0#> .
@prefix samm-u: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .

:BatteryTypeInformation a samm:Aspect ;
   samm:preferredName "Carbonfootprint"@en ;
   samm:description "Describes the carbonfootprint of the entity."@en ;
   samm:properties ( :energy :batteryCategory :powerCapability :resistanceInformation :batteryLifetime :roundTripEnergy :batteryChemistry :componentInformation ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:energy a samm:Property ;
   samm:preferredName "Energy"@en ;
   samm:description "This attribute describes energy related information about the batter."@en ;
   samm:characteristic :CharacteristicEnergy .

:batteryCategory a samm:Property ;
   samm:preferredName "Battery Category"@en ;
   samm:description "This Attribute describes the intended use of the battery"@en ;
   samm:characteristic :BatteryCategory .

:powerCapability a samm:Property ;
   samm:preferredName "Power Capability"@en ;
   samm:description "Contains all attributes associated with the Power Capability of the battery"@en ;
   samm:characteristic :PowerCapability .

:resistanceInformation a samm:Property ;
   samm:preferredName "Resistance Informatoin"@en ;
   samm:description "This attribute contains information about the resistance of this battery"@en ;
   samm:characteristic :ResistanceCharacteristic .

:batteryLifetime a samm:Property ;
   samm:preferredName "Battery Lifetime"@en ;
   samm:description "This attribute contains information on the battery lifetime"@en ;
   samm:characteristic :LifetimeCharacteristic .

:roundTripEnergy a samm:Property ;
   samm:preferredName "Round Trip Energy Information"@en ;
   samm:description "This attribute contains information about the self discharge properties and energy efficiency of the battery"@en ;
   samm:characteristic :RoundTripCharacteristic .

:batteryChemistry a samm:Property ;
   samm:preferredName "Battery Chemistry"@en ;
   samm:description "Short description of the main chemical components of the battery"@en ;
   samm:characteristic :CharacteristicBatteryChemistry ;
   samm:exampleValue "Lithium-Ion" .

:componentInformation a samm:Property ;
   samm:preferredName "Component Information"@en ;
   samm:description "Information on the main components anode, cathode an electrolyte"@en ;
   samm:characteristic :CharacteristicComponents .

:CharacteristicEnergy a samm:Characteristic ;
   samm:dataType :CapacityEnergySoHAndVoltageEntity .

:BatteryCategory a samm-c:Enumeration ;
   samm:preferredName "Battery Category"@en ;
   samm:dataType xsd:string ;
   samm-c:values ( "LMT battery" "ElecticVehicleBattery" "IndustrialBattery" "StationaryBatteryEnergyStorageSystem" "otherIndustrialBatteries" ) .

:PowerCapability a samm:Characteristic ;
   samm:preferredName "Power Capability"@en ;
   samm:dataType :PowerCapabilityEntitiy .

:ResistanceCharacteristic a samm:Characteristic ;
   samm:preferredName "Resistance"@en ;
   samm:dataType :InternalResistance .

:LifetimeCharacteristic a samm:Characteristic ;
   samm:dataType :BatteryLifetime .

:RoundTripCharacteristic a samm:Characteristic ;
   samm:dataType :RoundTrip .

:CharacteristicBatteryChemistry a samm:Characteristic ;
   samm:dataType xsd:string .

:CharacteristicComponents a samm:Characteristic ;
   samm:dataType :EntityComponents .

:CapacityEnergySoHAndVoltageEntity a samm:Entity ;
   samm:preferredName "Capacity, energy, SoH & Voltage"@en ;
   samm:description "Entity that describes the capacity, energy, SoH & Voltage."@en ;
   samm:properties ( :ratedCapacity :minimumVoltage :maximumVoltage :nominalVoltage ) .

:PowerCapabilityEntitiy a samm:Entity ;
   samm:properties ( :originalPowerCapability :maxPermittedBatteryPower :nominalToMaxPowerRatio ) .

:InternalResistance a samm:Entity ;
   samm:properties ( :internalCellResistance :internalModuleResistance :internalPackResistance ) .

:BatteryLifetime a samm:Entity ;
   samm:properties ( :expectedLifetime :minCapacity :soceThreshold ) .

:RoundTrip a samm:Entity ;
   samm:properties ( :initalSelfDischarge :initialRoundTripEfficiency ) .

:EntityComponents a samm:Entity ;
   samm:properties ( :anode :cathode :electrolyte ) .

:ratedCapacity a samm:Property ;
   samm:preferredName "Rated capacity"@en ;
   samm:description "Describes the rated capacity."@en ;
   samm:characteristic :UnitRelevantCharacteristic .

:minimumVoltage a samm:Property ;
   samm:preferredName "Minimum voltage"@en ;
   samm:description "Describes the minimum voltage."@en ;
   samm:characteristic :UnitRelevantCharacteristic .

:maximumVoltage a samm:Property ;
   samm:preferredName "Maximum voltage"@en ;
   samm:description "Describes the maximum voltage."@en ;
   samm:characteristic :UnitRelevantCharacteristic .

:nominalVoltage a samm:Property ;
   samm:preferredName "nominalVoltage"@en ;
   samm:description "nominalVoltage of the battery"@en ;
   samm:characteristic :UnitRelevantCharacteristic .

:originalPowerCapability a samm:Property ;
   samm:description "Original Power capability in Watt "@en ;
   samm:characteristic :UnitRelevantCharacteristic .

:maxPermittedBatteryPower a samm:Property ;
   samm:preferredName "Maximum Permitted Battery Power"@en ;
   samm:description "Maximum Permitted Battery Power in Watt"@en ;
   samm:characteristic :UnitRelevantCharacteristic .

:nominalToMaxPowerRatio a samm:Property ;
   samm:preferredName "Nominal to max powert Ratio"@en ;
   samm:description "Ratio between nominal allowed battery power  (Watt) and battery energy (Watt hours) regulation specifies % but ratio suggest unit hz."@en ;
   samm:characteristic :ValueCharatcteristic ;
   samm:exampleValue "12"^^xsd:float .

:internalCellResistance a samm:Property ;
   samm:preferredName "Internal Cell Resistance"@en ;
   samm:description "Internal Resistance on Cell Level in Ohm, this is a pre-use value"@en ;
   samm:characteristic :UnitRelevantCharacteristic .

:internalModuleResistance a samm:Property ;
   samm:preferredName "Internal Module Resistance"@en ;
   samm:description "Internal Resistance on Module level, measured in Ohm. This is a pre-use value"@en ;
   samm:characteristic :UnitRelevantCharacteristic .

:internalPackResistance a samm:Property ;
   samm:preferredName "Internal Pack Resistance"@en ;
   samm:description "Internal Resistance on a Pack level, measured on Ohm. Thi is a pre-use value."@en ;
   samm:characteristic :UnitRelevantCharacteristic .

:expectedLifetime a samm:Property ;
   samm:preferredName "Expected Lifetime "@en ;
   samm:description "Expected Lifetime of this battery time measured in charge-discharge cycles"@en ;
   samm:characteristic :ExpectedLifetime ;
   samm:exampleValue "2500"^^xsd:positiveInteger .

:minCapacity a samm:Property ;
   samm:preferredName "Capacity threshold for exhaustion"@en ;
   samm:description "Minimum Percentage of rated capacity, above which the battery is still considered operational"@en ;
   samm:characteristic :MinCapacityTrait ;
   samm:exampleValue 69 .

:soceThreshold a samm:Property ;
   samm:preferredName "SOCE Threshold"@en ;
   samm:description "SOCE Standard as metric for the exhaustion status of a EV battery"@en ;
   samm:characteristic :MinCapacityTrait ;
   samm:exampleValue 75 .

:initalSelfDischarge a samm:Property ;
   samm:preferredName "Initial Self-Discharging Rage"@en ;
   samm:description "Describes the Inital self discharging rate measured in % per month"@en ;
   samm:characteristic :MinCapacityTrait ;
   samm:exampleValue 5 .

:initialRoundTripEfficiency a samm:Property ;
   samm:preferredName "Initial Roundtrip Efficiency"@en ;
   samm:description "Energy round trip efficiency means the ratio of the net energy delivered by a battery during a discharge test to the total energy required to restore the initial state of charge by a standard charge"@en ;
   samm:characteristic :MinCapacityTrait ;
   samm:exampleValue 10 .

:anode a samm:Property ;
   samm:characteristic :CharacteristicComponent .

:cathode a samm:Property ;
   samm:characteristic :CharacteristicComponent .

:electrolyte a samm:Property ;
   samm:characteristic :CharacteristicComponent .

:UnitRelevantCharacteristic a samm:Characteristic ;
   samm:preferredName "UnitRelevantCharacteristic"@en ;
   samm:description "Describes a value which is unit sensitive e.g. the nominal value of a battery"@en ;
   samm:dataType :Value .

:ValueCharatcteristic a samm:Characteristic ;
   samm:preferredName "value"@en ;
   samm:description "This value is a SI value Such as capacity, voltage etc"@en ;
   samm:dataType xsd:float .

:ExpectedLifetime a samm:Characteristic ;
   samm:dataType xsd:positiveInteger .

:MinCapacityTrait a samm-c:Trait ;
   samm-c:baseCharacteristic :PercentageCharacteristic ;
   samm-c:constraint :Constraint1 .

:CharacteristicComponent a samm:Characteristic ;
   samm:dataType :EntityComponent .

:Value a samm:Entity ;
   samm:preferredName "Value"@en ;
   samm:description "Value"@en ;
   samm:properties ( :unit :value ) .

:PercentageCharacteristic a samm:Characteristic ;
   samm:preferredName "PercentageCharacteristic"@en ;
   samm:description "Characteristic to describe a percentage value within 0-100"@en ;
   samm:dataType xsd:integer .

:Constraint1 a samm-c:RangeConstraint ;
   samm-c:minValue 0 ;
   samm-c:maxValue 100 ;
   samm-c:lowerBoundDefinition samm-c:AT_LEAST ;
   samm-c:upperBoundDefinition samm-c:AT_MOST .

:EntityComponent a samm:Entity ;
   samm:properties ( :precisionRange :massPercentage :materialIdentifier ) .

:unit a samm:Property ;
   samm:preferredName "unit"@en ;
   samm:description "si unit of the related value"@en ;
   samm:characteristic samm-c:UnitReference .

:value a samm:Property ;
   samm:preferredName "value"@en ;
   samm:description "relevant value"@en ;
   samm:characteristic :ValueCharatcteristic ;
   samm:exampleValue "4.20"^^xsd:float .

:precisionRange a samm:Property ;
   samm:preferredName "Precision Range"@en ;
   samm:description "Use this attribute to specify the precision "@en ;
   samm:characteristic :PrecisionRangeTrait ;
   samm:exampleValue ".1"^^xsd:float .

:massPercentage a samm:Property ;
   samm:characteristic :MassPercentageTrait ;
   samm:exampleValue "45"^^xsd:float .

:materialIdentifier a samm:Property ;
   samm:preferredName "CAS-Number of referenced Component"@en ;
   samm:description "Use one of the critical raw materials listed in the link below"@en ;
   samm:see <https://single-market-economy.ec.europa.eu/sectors/raw-materials/areas-specific-interest/critical-raw-materials_en> ;
   samm:characteristic :MaterialIdentifierTrait ;
   samm:exampleValue "1234-567-89" .

:PrecisionRangeTrait a samm-c:Trait ;
   samm-c:baseCharacteristic :Characteristic1 ;
   samm-c:constraint :ConstraintPrecisionFloat .

:MassPercentageTrait a samm-c:Trait ;
   samm-c:baseCharacteristic :CharacteristicMassPercentage ;
   samm-c:constraint :Constraint3 .

:MaterialIdentifierTrait a samm-c:Trait ;
   samm-c:baseCharacteristic :CharacteristicCAS ;
   samm-c:constraint :ConstraintCAS .

:Characteristic1 a samm:Characteristic ;
   samm:dataType xsd:float .

:ConstraintPrecisionFloat a samm-c:RangeConstraint ;
   samm-c:minValue "0"^^xsd:float ;
   samm-c:maxValue "1"^^xsd:float ;
   samm-c:lowerBoundDefinition samm-c:AT_LEAST ;
   samm-c:upperBoundDefinition samm-c:AT_MOST .

:CharacteristicMassPercentage a samm:Characteristic ;
   samm:dataType xsd:float .

:Constraint3 a samm-c:RangeConstraint ;
   samm-c:minValue "0"^^xsd:float ;
   samm-c:maxValue "100"^^xsd:float ;
   samm-c:lowerBoundDefinition samm-c:AT_LEAST ;
   samm-c:upperBoundDefinition samm-c:AT_MOST .

:CharacteristicCAS a samm:Characteristic ;
   samm:dataType xsd:string .

:ConstraintCAS a samm-c:RegularExpressionConstraint ;
   samm:value "[0-9]{2-7}-[0-9]{3}-[0-9]{2}" .

